{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
    	"project": "./tsconfig.json"
	},
	"plugins": ["@typescript-eslint", "prettier"],
	"rules": {
		"prettier/prettier": 2,
		"no-console": 0
	},
}
